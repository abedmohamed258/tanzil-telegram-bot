{
  "name": "tanzil-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot for downloading videos from various platforms using yt-dlp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "clean": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true })\""
  },
  "keywords": [
    "telegram",
    "bot",
    "video",
    "download",
    "yt-dlp"
  ],
  "author": "Abed Mohamed",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.27.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "input": "^1.0.1",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.22",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "fast-check": "^4.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.7.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.48.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "overrides": {
    "form-data": "4.0.4",
    "tough-cookie": "4.1.4"
  }
}
